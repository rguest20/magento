	<cms_page>
		<title>Home Page Version 5</title>
		<identifier>home-page-version-5</identifier>
		<page_layout>1column</page_layout>
		<content><![CDATA[<!-- SECTION 1 -->
<div class="container">
<div class="row">
<div class="col-md-3 col-xs-12"><!-- TOP CATEGORIES -->
<div class="margin-top20">{{widget type="MGS\Mpanel\Block\Widget\CategoryNavigation" title="Top Categories" category_id="20,11,41,47,48,49"}}</div>
<!-- TRENDING MENU -->
<div class="margin-top20">{{widget type="Magento\Cms\Block\Widget\Block" template="widget/static_block/default.phtml" block_id="orson_home_5_trending_menu"}}</div>
</div>
<!-- SLIDER -->
<div class="col-md-9 col-xs-12">
<div class="margin-top20">{{widget type="MGS\Mpanel\Block\Widget\Slider"}}</div>
</div>
</div>
</div>
<hr class="margin-top20 margin-bottom20" /><!-- SECTION 2 -->
<div class="container">
<div class="row"><!-- SERVICE 1 -->
<div class="col-md-4 col-sm-6 col-xs-12">{{widget type="Magento\Cms\Block\Widget\Block" template="widget/static_block/default.phtml" block_id="orson_home_5_service_1"}}</div>
<!-- SERVICE 2 -->
<div class="col-md-4 col-sm-6 col-xs-12">{{widget type="Magento\Cms\Block\Widget\Block" template="widget/static_block/default.phtml" block_id="orson_home_5_service_2"}}</div>
<!-- SERVICE 3 -->
<div class="col-md-4 col-sm-12 col-xs-12">{{widget type="Magento\Cms\Block\Widget\Block" template="widget/static_block/default.phtml" block_id="orson_home_5_service_3"}}</div>
</div>
</div>
<!-- SECTION 3 NEW ARRIVALS -->
<div class="container">
<div class="padding-top15 owl-top-text icon-horizontal">{{widget type="MGS\Mpanel\Block\Widget\ProductList" title="New Arrivals" view_mode="mode_grid" type_list="new" slider_mode_grid="1" per_row_grid="5" number_show="7"}}</div>
</div>
<!-- SECTION 4 PROMO BANNER GROUP -->
<div class="container">
<div class="padding-top40 padding-bottom35">
<div class="row">
<div class="col-md-3 col-sm-4 col-xs-6">{{widget type="MGS\Promobanners\Block\Widget\Banner" banner_id="orson_home_5_banner_1" template="banner.phtml"}}</div>
<div class="col-md-3 col-sm-4 col-xs-6">{{widget type="MGS\Promobanners\Block\Widget\Banner" banner_id="orson_home_5_banner_2" template="banner.phtml"}}</div>
<div class="col-md-12 col-xs-12 visible-xs">
<div class="separator" style="padding-top: 30px;">&nbsp;</div>
</div>
<div class="col-md-3 col-sm-4 col-xs-6">{{widget type="MGS\Promobanners\Block\Widget\Banner" banner_id="orson_home_5_banner_3" template="banner.phtml"}}</div>
<div class="col-md-3 hidden-sm col-xs-6">{{widget type="MGS\Promobanners\Block\Widget\Banner" banner_id="orson_home_5_banner_4" template="banner.phtml"}}</div>
</div>
</div>
</div>
<!-- SECTION 5 -->
<div class="container">
<div class="padding-bottom5">
<div class="row"><!-- ON SALE -->
<div class="col-md-6 col-xs-12 owl-top-text icon-horizontal margin-bottom35">{{widget type="MGS\Mpanel\Block\Widget\ProductList" title="On Sale" view_mode="mode_grid" type_list="sale" slider_mode_grid="1" per_row_grid="3" number_show="5"}}</div>
<!-- TOP RATED -->
<div class="col-md-6 col-xs-12 owl-top-text icon-horizontal margin-bottom35">{{widget type="MGS\Mpanel\Block\Widget\ProductList" title="Top Rated" view_mode="mode_grid" type_list="top_rate" slider_mode_grid="1" per_row_grid="3" number_show="5"}}</div>
</div>
</div>
</div>
<!-- SECTION 6 -->
<div class="padding-top35 padding-bottom5" style="background-color: #eeeeee;">
<div class="container">{{widget type="Magento\Cms\Block\Widget\Block" template="widget/static_block/default.phtml" block_id="orson_home_5_service_4"}}</div>
</div>
<!-- SECTION 7 -->
<div class="container">
<div class="padding-top35 owl-top-text" style="padding-bottom: 50px;">{{widget type="MGS\Brand\Block\Widget\Brand" brand_ids="9,10,11,12,13,14,15" title="Our Clients" view_as="owl_carousel" items="5" image_width="240" image_height="90" navigation="1" template="widget/default.phtml"}}</div>
</div>]]></content>
	</cms_page>
	
