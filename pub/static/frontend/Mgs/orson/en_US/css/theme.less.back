// Massage
.message {
	border: 1px solid transparent;
    margin-bottom: 20px;
    padding: 15px 15px 15px 55px;
	font-size: 13px;
    color: #fff;
	position: relative;
	background: #5ec2e7;
	border-color: #5ec2e7;
	.border-radius(4px);
	&:after {
		font-family: FontAwesome;
		font-size: 20px;
		color: #fff;
		position: absolute;
		top: 50%;
		.transform(translateY(-50%));
		left: 24px;
		content: "\f024";
	}
	&.empty, &.warning, &.danger, &.notice {
		background-color: #efba48;
		border-color: #efba48;
	}
	&.empty:after, &.warning:after, &.danger:after, &.notice:after {
		content: "\f12a";
	}
	&.error {
		background-color: #d85e5e;
		border-color: #d85e5e;
		&:after {
			content: "\f00c";
		}
	}
	&.success {
		background-color: #00b174;
		border-color: #00b174;
		&:after {
			content: "\f00c";
		}
	}
}
