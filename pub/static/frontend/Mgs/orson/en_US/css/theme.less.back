// SHORT CODE
.no-padding  {
	padding: 0;
}
*,*:hover, *:focus {
	outline: none !important;
}
//-------------- Body --------------
body { 
	color:#858585; 
	background: #fff;
	text-align:left; 
	font-weight: 400; 
	font-size: 14px; 
	line-height: 1.5;
	#maincontent {
		min-height: 600px;
		> .row {
			width: auto;
		}
	}
}
.cms-index-index {
	.heading-page1 {
		display: none;
	}
	.page-wrapper  > #maincontent {
		width: 100%;
		padding: 0;
	}
	.page-main > .page.messages {
		position: absolute;
		top: 0;
		left: 0;
		right: 0;
		z-index: 999;
	}
	.footer { margin: 0; }
	.header-v2{
		.vertical-menu-home .menu-title .icon-vertical{display: none;}
	}
}
html, body { overflow-x:hidden } 

ul,ol {
	list-style: none;
	padding: 0;
	margin: 0;
	li {
		padding: 5px 0;
	}
}
a, button {
	.transition(all 300ms ease-in-out);
}
.table {
	width: 100%;
	tr td, tr th {
		border: 1px solid #ddd;
		text-align: center;
		padding: 8px 10px;
		vertical-align: middle;
	}
	thead tr th {
		color: #222;
		font-weight: 700;
		text-transform: uppercase;
	}
}

h1, .h1 { font-size: 28px; }
h2, .h2 { font-size: 24px; }
h3, .h3 { font-size: 20px; }
h4, .h4 { font-size: 16px; }
h5, .h5 { font-size: 15px; }
h6, .h6 { font-size: 14px; }
a  {  
	text-decoration:none; color: #222222; outline: none;
	&:hover, &:focus {
		text-decoration:none; color: @base-color; outline: none;
	}
}
//-------------- Form --------------
form          { display:inline; }
fieldset      { border:0; }
.form-control, .input-text, input[type="text"], input[type="password"], input[type="email"], select, textarea {
	background-color: #fff;
	height: 36px;
	padding-left: 18px;
	border: 1px solid #dddddd;
	width: 100%;
	color: #868686;
	.border-radius(4px);
}
select[multiple], select[size] {
	min-height: 144px;
	padding-left: 0;
}
select[multiple] option {
	height: 36px;
	padding-left: 18px;
	padding-top: 8px;
}
textarea {
	min-height: 130px;
}
.form-group label {
	font-weight: 500;
	color: #222;
}
.form-control, .input-text, input[type="text"], input[type="password"], input[type="email"] {
	padding-left: 18px;
}
.radio input[type="radio"], .radio-inline input[type="radio"], .checkbox input[type="checkbox"], .checkbox-inline input[type="checkbox"] {
	margin-top: 1px;
}
//
input[type="number"] {
    -moz-appearance: textfield;
}

input[type="number"]::-webkit-inner-spin-button,input[type="number"]::-webkit-outer-spin-button {
    -webkit-appearance: none;
    margin: 0;
}
.mage-error {
	color: red;
}
.no-padding-left{
	padding-left: 0;
}
//------------------- BUTTON -------------------
button:hover, button:focus { outline: none !important; }
.btn {
	text-transform: uppercase;
	font-size: 13px;
	font-weight: 700;
	.border-radius(4px);
}
.btn-lg {
	padding: 12px 19px;
}
.btn-sm {
	padding: 7px 21px;
}
.btn-default {
	background: #eeeeee;
	color: #212121;
	border: 1px solid #eeeeee;
	&:hover, &:focus {
		background: @base-color;
		color: #212121;
		border: 1px solid @base-color;
	}
}
.btn-primary {
	background: @base-color;
	color: #212121;
	border: 1px solid @base-color;
	&:hover, &:focus {
		color: #fff;
		border: 1px solid @base-color;
		background: @base-color;
	}
}
.btn-secondary {
	background: #444444;
	color: #fff;
	border: 1px solid #444444;
	&:hover, &:focus {
		background: #eeeeee;
		color: #212121;
		border: 1px solid #eeeeee;
	}
}
.btn-icon {
	padding: 0;
	overflow: hidden;
	.icon {
		float: left;
		display: inline-block;
		background: rgba(0,0,0,0.3);
		width: 36px;
		height: 36px;
		line-height: 36px;
		text-align: center;
	}
	.text {
		line-height: 36px;
		text-align: center;
		padding: 0 15px;
	}
}
.btn-primary.disabled, .btn-primary[disabled], fieldset[disabled]  .btn-primary, .btn-primary.disabled:hover, .btn-primary[disabled]:hover, fieldset[disabled] .btn-primary:hover, .btn-primary.disabled:focus, .btn-primary[disabled]:focus, fieldset[disabled] .btn-primary:focus, .btn-primary.disabled:active, .btn-primary[disabled]:active, fieldset[disabled] .btn-primary:active, .btn-primary.disabled.active, .btn-primary.active[disabled], fieldset[disabled] .btn-primary.active {
	background-color: @base-color;
	border-color: @base-color;
}
/*-------------- Price --------------*/
.price {
	font-size: 18px;
	color: #ef5254;
	font-weight: bold;
}
.old-price .price {
	font-size: 14px;
	color: #858585;
	text-decoration: line-through;
}
.price-box > p {
	margin: 0;
	display: inline-block;
}
.price-box > p.old-price {
	padding-right: 5px;
}
// MGS OWL SLIDER
.slider_mgs_carousel {
	&.owl-theme {
		.owl-controls {
			margin: 0;
			.owl-nav {
				[class*="owl-"] {
					position: absolute;
					top: 50%;
					border: 1px solid #222;
					width: 36px;
					height: 36px;
					font-size: 16px;
					color: #fff;
					background: #222;
					text-align: center;
					line-height: 32px;
					padding: 0;
					margin: 0;
					opacity: 0;
					.transform(translateY(-50%));
					.border-radius(4px);
					.transition(all 300ms ease-in-out);
					&:hover {
						border-color: #222;
						background: #fff;
						color: #222;
					}
				}
				.owl-next { right: 20px !important; }
				.owl-prev { left: 20px !important; }
			}
		}
		.owl-dots {
			position: absolute;
			bottom: 0;
			left: 0;
			right: 0;
			span {
				background: #222;
				margin: 5px !important;
				.transition(all 300ms ease-in-out);
			}
			.owl-dot {
				&.active , &:hover {
					span {
						background: @base-color;
					}
				}
			}
		}
		&:hover {
			.owl-controls .owl-nav [class*="owl-"] {
				opacity: 1;
			}
		}
	}
}
.home-subscribe {
	.block-content {
		display: table;
		width: 100%;
		.input-box {
			width: 270px;
			float: left;
			.input-text {
				height: 44px;
			}
		}
		.actions {
			float: left;
			margin-left: 10px;
		}
	}
}
.center-content {
	.title-content { text-align: center }
}
.title-content {
    text-transform: uppercase;
	.title {
		color: #444444;
		display: inline-block;
		font-weight: 700;
		margin: 0;
		padding: 0 2px 5px;
		position: relative;
		&:after {
			background: @base-color;
			bottom: -1px;
			content: "";
			height: 2px;
			left: 0;
			position: absolute;
			width: 100%;
		}
	}
}
/********** Product Grid **********/
.on-wishlist .fa:before {
	content: "\f004";
	color: @base-color;
}
.on-wishlist:hover .fa:before {
	color: #fff;
}
.products-grid {
	.owl-carousel-nopadding .owl-item {
		padding: 0 !important;
	}
	li {
		padding-top: 0;
		padding-bottom: 0;
	}
	.item {
		margin-bottom: 30px;
	}
	.product-content {
		.product-top {
			position: relative;
			border: 1px solid #dddddd;
			> a {
				display: block;
			}
			.img-base {
				position: absolute; top: 0; left: 0; right: 0; bottom: 0; opacity: 0;
			}
			.icon-links {
				position: absolute;
				right: 20px;
				bottom: 20px;
				li {
					margin-bottom: 5px;
					padding: 0;
					&:last-child {
						margin-bottom: 0;
					}
					button {
						box-shadow: 0 0 2px 0 rgba(0, 0, 0, 0.4);
						background: #fff;
						width: 40px;
						height: 40px;
						border: 1px solid #fff;
						color: #222222;
						font-size: 14px;
						visibility: hidden;
						opacity: 0;
						.transform(rotateY(90deg));
						.border-radius(4px);
						&:hover {
							background-color: @base-color;
							border-color: @base-color;
						}
					}
				}
			}
		}
		&:hover {
			.product-top {
				.img-base {
					opacity: 1;
				}
			}
			.product-top .icon-links button {
				opacity: 1;
				visibility: visible;
				.transform(rotateY(0deg));
			}
		}
		.product-desc {
			padding-top: 30px;
			.cate-name {
				display: none;
				font-size: 13px;
				font-weight: 700;
				color: #999999;
				margin-bottom: 9px;
				text-transform: uppercase;
			}
			.product-name {
				margin-top: 0;
				margin-bottom: 10px;
				font-weight: 400;
			}
			.price-box {
				display: table-cell;
				vertical-align: middle;
			}
			.product-reviews-summary {
				display: table-cell;
				width: 65px;
				vertical-align: middle;
				.reviews-actions {
					display: none;
				}
				.rating-summary .label {
					display: none;
				}
			}
			.tocart {
				width: 100%;
				height: 36px;
				background: #aaaaaa;
				border: none;
				line-height: 36px;
				text-transform: uppercase;
				font-weight: 700;
				overflow: hidden;
				color: #fff;
				.border-radius(4px);
				.icon {
					font-size: 12px;
					.transition(all 300ms ease-in-out 0s);
				}
				&:hover {
					background: @base-color;
				}
				.text {
					padding: 0;
					text-align: center;
				}
			}
			.prar {
				margin-bottom: 14px;
				display: table;
				width: 100%;
				.price-box {
					.price-label {
						display: none;
					}
				}
			}
		}
	}
	.owl-item {
		padding: 0 10px;
		.product-top {
			border-right: 1px solid transparent;
			> a {
				border-right: 1px solid #ddd;
			}
		}
		.item {
			margin-bottom: 0;
		}
	}
}
.product-label {
	height: 26px;
	position: absolute;
	top: 10px;
	text-transform: uppercase;
	color: #fff;
	padding: 2px 9px;
	z-index: 1;
	font-weight: 500;
	&.sale-label {
		background: #6fa2f7;
		right: -2px;
		border-top-left-radius: 3px;
		border-bottom-left-radius: 3px;
		-ms-border-top-left-radius: 3px;
		-ms-border-bottom-left-radius: 3px;
		-moz-border-top-left-radius: 3px;
		-moz-border-bottom-left-radius: 3px;
		-webkit-border-top-left-radius: 3px;
		-webkit-border-bottom-left-radius: 3px;
		-o-border-top-left-radius: 3px;
		-o-border-bottom-left-radius: 3px;
	}
	&.new-label {
		background: #ef7e7e;
		left: -2px;
		border-top-right-radius: 3px;
		border-bottom-right-radius: 3px;
		-ms-border-top-right-radius: 3px;
		-ms-border-bottom-right-radius: 3px;
		-moz-border-top-right-radius: 3px;
		-moz-border-bottom-right-radius: 3px;
		-webkit-border-top-right-radius: 3px;
		-webkit-border-bottom-right-radius: 3px;
		-o-border-top-right-radius: 3px;
		-o-border-bottom-right-radius: 3px;
	}
